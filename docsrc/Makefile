all: userman devman

# run doxygen twice to pull in the header generated by build.sh
userman: ../src/stagecpp.cc help.txt build.sh stage.dox
	./build.sh
	doxygen stage.dox
	./build.sh
	doxygen stage.dox

devman: ../src/*.c ../src/*.h ../src/*.cc help.txt libstage.dox
	doxygen libstage.dox


clean:
	rm -rf user developer
