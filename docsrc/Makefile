all: userman refman

# run doxygen twice to pull in the header generated by build.sh
userman: ../src/stagecpp.cc *.txt build.sh stage_user.dox
	./build.sh
	doxygen stage_user.dox
	./build.sh
	doxygen stage_user.dox
	cp stage_snip.png stage_user

refman: ../src/*.c ../src/*.h ../src/*.cc *.txt stage_reference.dox
	doxygen stage_reference.dox
	cp stage_snip.png stage_reference


clean:	
	rm -rf stage_user stage_reference
