all: userman devman

# run doxygen twice to pull in the header generated by build.sh
userman: ../src/stagecpp.cc help.txt build.sh stage_user.dox
	./build.sh
	doxygen stage_user.dox
	./build.sh
	doxygen stage_user.dox

devman: ../src/*.c ../src/*.h ../src/*.cc help.txt stage_developer.dox
	doxygen stage_developer.dox


clean:
	rm -rf user developer
