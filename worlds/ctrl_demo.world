include "pioneer.inc"
include "map.inc"
include "sick.inc"

window
( 
  size [ 500.000 500.000 ] 
  center [-6 -7] 
  rotate [ 0.000 0.000 ]
  scale 26 
)

floorplan
( 
  name "cave"
  size3 [16 16 0.5]
  pose [0.000 0.000 0.000]
  bitmap "bitmaps/cave.png"
)

pioneer2dx
(
  pose [ -7.000 -7.000 45.000 ]
  sicklaser
  ( 
     ctrl "lasernoise" 
  ) 
)

