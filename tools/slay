
#/bin/bash

HOSTS=nw0:nw4
PROGS="stage player xs"

# set the separator
IFS=:

# use $1 for options to killall (-9 for example)

for HOST in $HOSTS
do
    echo $HOST
#	ssh $HOST -uname -a

    # kill the programs
    ssh $HOST /usr/bin/killall $1 $PROGS

done

#local kill
echo localhost
killall $1 stage player xs
